#ifdef GL_ES
precision highp float;
#endif
attribute vec4 vertex;
attribute vec4 color;
attribute vec2 tex_coord, tex_coord2, tex_coorda;
uniform mat4 mvp;
varying vec4 col;
varying vec2 tex_c, tex_cuv, tex_a;
void main()
{
   gl_Position = mvp * vertex;
   col = color;
   tex_c = tex_coord;
   tex_cuv = tex_coord2 * 0.5;
   tex_a = tex_coorda;
}
