import ecore_types;

class Callback (Eo.Base)
{
   data: Callback_Data;
   methods {
      onecallback {
         params {
           @in cb: Ecore_Cb;
           @in data: void_ptr;
         }
      }
      twocallback {
         params {
           @in cb: Ecore_Cb;
           @in data: void_ptr;
           @in cb2: Ecore_Cb;
         }
      }
      test_global_callbacks @class {
         params {
           @in cb: Ecore_Cb;
           @in data: void_ptr;
         }
      }
   }
   implements {
      Eo.Base.constructor;
   }
   events {
     call_on_add;
   }
}
